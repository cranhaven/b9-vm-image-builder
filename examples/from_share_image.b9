Project {
  projectName = "from_shared_to_shared",
  projectDisks = [
    (ImageTarget
      (Share "${arg_1}_with_emacs" QCow2 KeepSize)
      (From "${arg_1}" KeepSize)
      (MountPoint "/"))
  ],
  projectSharedDirectories = [],
  projectBuildScript =
    Begin [
      Run "pacman" ["--noconfirm", "-S", "emacs"]
    ],
  projectCpuArch = X86_64
}